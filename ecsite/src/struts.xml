		<action name="AdminAction" class="com.internousdev.ecsite.action.AdminAction"
			method="execute">
			<result name="success">admin.jsp</result>

		</action>

		<action name="ItemCreateAction" class="com.internousdev.ecsite.action.ItemCreateAction"
